#include<bits/stdc++.h>
using namespace std;

#define ll long long

int main()
{
    ll t;
    cin>>t;

    while(t--)
    {
        ll n,q;
        cin>>n >> q;

        vector<ll >arr;
        ll sum=0;

        for(int i=0;i<n;i++)
        {
            ll x;
            cin>>x;
            arr.push_back(x);
            sum += x;
        }

        map<ll,ll>m;

        for(int i=0;i<n;i++)
        {
            m[arr[i]]=i;
        }

        sort(arr.begin(),arr.end());

        while(q--)
        {
            ll x;
            cin>>x;

            auto to_find= upper_bound(arr.begin(),arr.end(),(sum-x));

            if( to_find == arr.end() && m.find(sum-x) != m.end())
                cout<<m[sum-x]+1<<"\n";

            else if(to_find != arr.end())
                cout<<m[*to_find]+1<<"\n";

            else
                cout<<"-1\n";
        }


    }
}
